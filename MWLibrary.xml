<MWElements>
    <MWElement name="Eye Calibrator 3D">
        <match_signature>calibrator[@type="standard_eye_calibrator_3d"]</match_signature>

        <isa>Calibrator</isa>

        <description>
            An I/O calibrator specially tailored for 3-D eye movement signals.
        </description>
        
        <code>
            <calibrator type="standard_eye_calibrator_3d" tag="New Eye Calibrator 3D"
                                                          eyeh_raw=""
                                                          eyev_raw=""
                                                          eyed_raw=""
                                                          eyeh_calibrated=""
                                                          eyev_calibrated=""
                                                          eyed_calibrated=""/>
        </code>
    </MWElement>
    <MWElement name="Linear Eye Calibrator 3D">
        <match_signature>calibrator[@type="linear_eye_calibrator_3d"]</match_signature>

        <isa>Calibrator</isa>

        <description>
            An I/O calibrator specially tailored for 3-D eye movement signals.
        </description>
        
        <code>
            <calibrator type="linear_eye_calibrator_3d" tag="New Linear Eye Calibrator 3D"
                                                        eyeh_raw=""
                                                        eyev_raw=""
                                                        eyed_raw=""
                                                        eyeh_calibrated=""
                                                        eyev_calibrated=""
                                                        eyed_calibrated=""/>
        </code>
    </MWElement>
    <MWElement name="Fake Calibratable Object 3D">
        <match_signature>calibrator[@type="fake_calibratable_object_3d"]</match_signature>

        <isa>Calibrator</isa>

        <description>
            A fake calibratable object used for testing 3-D calibrators.
        </description>
        
        <code>
            <calibrator type="fake_calibratable_object_3d" tag="New Fake Calibratable Object 3D"
                                                           x_value=""
                                                           y_value=""
                                                           z_value=""/>
        </code>
    </MWElement>
</MWElements>
